<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.lizec.smartreview.mapper.AppTypeDao">


    <select id="selectOne" resultType="top.lizec.smartreview.entity.AppType">
        SELECT id, name, comp FROM app_type
        WHERE id=#{id}
    </select>

    <select id="selectAll" resultType="top.lizec.smartreview.entity.AppType">
        SELECT id, name, comp FROM app_type;
    </select>
</mapper>